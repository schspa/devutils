      <reg_short_name>PMCID2SR</reg_short_name>
      <reg_long_name>CONTEXTIDR_EL2 Sample Register</reg_long_name>
        <reg_condition otherwise="RES0">when ARMv8.2-PCSample is implemented and EL2 is implemented</reg_condition>
      
  <reg_address
