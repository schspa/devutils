          <reg_mapping>
              
            <mapped_name filename="ext-edeccr.xml">EDECCR</mapped_name>
            <mapped_type>Architectural</mapped_type>
              <mapped_execution_state>External</mapped_execution_state>
              <mapped_from_startbit>31</mapped_from_startbit>
