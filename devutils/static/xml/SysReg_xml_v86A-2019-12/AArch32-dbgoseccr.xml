      <reg_purpose>
        
    
      <purpose_text>
        <para>Provides a mechanism for an operating system to access the contents of <register_link state="ext" id="ext-edeccr.xml">EDECCR</register_link> that are otherwise invisible to software, so it can save/restore the contents of <register_link state="ext" id="ext-edeccr.xml">EDECCR</register_link> over powerdown on behalf of the external debugger.</para>
      </purpose_text>

      </reg_purpose>
      <reg_groups>
          <reg_group>Debug registers</reg_group>
      </reg_groups>
      <reg_configuration>
        
    
      <configuration_text>
        <para>If DBGOSLSR.OSLK == 0 then DBGOSECCR returns an <arm-defined-word>UNKNOWN</arm-defined-word> value on reads and ignores writes.</para>
      </configuration_text>

      </reg_configuration>
      <reg_attributes>
          <attributes_text>
            <para>DBGOSECCR is a 32-bit register.</para>
          </attributes_text>
      </reg_attributes>
