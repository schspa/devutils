      <reg_short_name>DBGOSECCR</reg_short_name>
      <reg_long_name>Debug OS Lock Exception Catch Control Register</reg_long_name>
        <reg_condition otherwise="UNKNOWN">when AArch32 is supported at any Exception level</reg_condition>
      

          <reg_reset_value></reg_reset_value>
      <reg_mappings>
          <reg_mapping>
              
            <mapped_name filename="AArch64-oseccr_el1.xml">OSECCR_EL1</mapped_name>
