<register_page>
  <registers>
    <register is_register="True" is_internal="False" is_banked="False" is_optional="False" is_stub_entry="False">
      <reg_short_name>MPAMCFG_PRI</reg_short_name>
      <reg_long_name>MPAM Priority Partition Configuration Register</reg_long_name>
        <reg_condition otherwise="RES0">when MPAMF_IDR.HAS_PRI_PART == 1</reg_condition>
      
  <reg_address
